generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Session {
  id           String   @id @default(uuid())
  created_at   DateTime @default(now())
  owner        String
  joined_users String[]
  current_song String
  queue        String[]
  isPlaying    Boolean
}
